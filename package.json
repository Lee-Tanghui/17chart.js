{
  "name": "17chart.js",
  "private": false,
  "version": "0.0.5-aplha",
  "keywords": [
    "echarts",
    "chart",
    "17chart"
  ],
  "scripts": {
    "dev": "umi dev",
    "build:chart": "rm -rf ./lib/ && rollup --config rollup.config.js",
    "build:dev": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "files": [
    "17chart",
    "lib",
    "README.md",
    "package.json"
  ],
  "main": "lib/esm/17chart.esm.js",
  "dependencies": {
    "echarts": "^5.3.2"
  },
  "devDependencies": {
    "@ant-design/pro-layout": "^6.38.8",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.25",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-markdown": "^8.0.3",
    "rollup": "^2.75.7",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.2",
    "umi": "^3.5.25",
    "yorkie": "^2.0.0"
  }
}
